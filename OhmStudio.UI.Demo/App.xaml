<Application x:Class="OhmStudio.UI.Demo.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:OhmStudio.UI.Demo.Views"
             StartupUri="Views/MainWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <local:XamlThemeResource />

                <ResourceDictionary Source="/OhmStudio.UI;Component/Styles/VisualStudio.xaml" />
                <ResourceDictionary Source="/GlobalStyle.xaml" />

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application> 